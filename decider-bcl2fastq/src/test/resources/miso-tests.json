{
    "defaults": {
        "description": "Bcl2fastq decider test",
        "metricsResources": "/expected_output/development/",
        "parameters": {
            "--pinery-url": "${misoPineryUrl}",
            "--run-scanner-url": "${runScannerUrl}",
            "--no-null-created-date": "",
            "--ignore-previous-lims-keys": "true"
        },
        "properties": {
            "memory": "16384"
        },
        "iniExclusions": ["parent-accessions", "workflow_run_accession", "workflow-run-accession",
            "unique_string", "parent_accessions", "parent_accession", "input_file", "input_files", "workflow_bundle_dir", "1_sort_order"],
        "iniStringSubstitutions": {
            ",\\d+,": ",SAMPLE_IUS_SWID,",
            ",\\d+:": ",LANE_IUS_SWID:"
        }
    },
    "tests": [
        {
            "id": "Mixed barcode length exclude sample test",
            "workflowName": "CASAVA",
            "workflowVersion": "2.7.1",
            "parameters": {
                "--include-lane": "170627_D00343_0178_ACB0F2ANXX_lane_8",
                "--exclude-sample": "GCONT_0004_Li_C_PE_264_TS"
            }
        },
        {
            "id": "Mixed barcode length include sample test",
            "workflowName": "CASAVA",
            "workflowVersion": "2.7.1",
            "parameters": {
                "--include-lane": "170627_D00343_0178_ACB0F2ANXX_lane_8",
                "--include-sample": "GCONT_0004_Li_C_PE_264_TS",
                "--demux-single-sample": ""
            }
        },
        {
            "id": "Override bases mask test",
            "workflowName": "CASAVA",
            "workflowVersion": "2.7.1",
            "parameters": {
                "--include-lane": "180420_E00389_0182_AHKTJLCCXY_lane_1",
                "--override-run-bases-mask": "y*,i8,y*"
            }
        },
        {
            "id": "Override bases mask test2",
            "workflowName": "CASAVA",
            "workflowVersion": "2.7.1",
            "parameters": {
                "--include-lane": "180420_E00389_0182_AHKTJLCCXY_lane_1",
                "--override-run-bases-mask": "y*,i6,i6,y*"
            }
        },
        {
            "id": "Mixed barcode length",
            "workflowName": "CASAVA",
            "workflowVersion": "2.7.1",
            "parameters": {
                "--include-lane": "170627_D00343_0178_ACB0F2ANXX_lane_8"
            }
        },
        {
            "id": "Mixed barcode length with override bases mask",
            "workflowName": "CASAVA",
            "workflowVersion": "2.7.1",
            "parameters": {
                "--include-lane": "170627_D00343_0178_ACB0F2ANXX_lane_8",
                "--override-run-bases-mask": "y*,i8,y*"
            }
        },
        {
            "id": "Barcode collision test",
            "workflowName": "CASAVA",
            "workflowVersion": "2.9.1",
            "parameters": {
                "--include-sequencer-run": "180507_M00753_0046_000000000-D3T43",
                "--min-allowed-edit-distance": "2"
            },
            "properties": {
                "mismatches": "0"
            }
        },
        {
            "id": "Force dual-barcode trim test",
            "workflowName": "CASAVA",
            "workflowVersion": "2.7.1",
            "parameters": {
                "--include-lane": "180420_D00343_0233_ACCH40ANXX_lane_7",
                "--override-run-bases-mask": "y*,i8n*,n*,y*"
            }
        },
        {
            "id": "No lane splitting one lane test",
            "workflowName": "CASAVA",
            "workflowVersion": "2.9.1",
            "parameters": {
                "--include-sequencer-run": "171128_NB551056_0036_AHVCJKBGX3",
                "--lane-splitting": "false"
            }
        },
        {
            "id": "No lane splitting multiple lanes test",
            "workflowName": "CASAVA",
            "workflowVersion": "2.9.1",
            "parameters": {
                "--include-sequencer-run": "180713_A00469_0010_AH7CT5DSXX",
                "--exclude-sample": "NOVA_0032_nn_U_PE_376_WG",
                "--lane-splitting": "false"
            }
        }, {
            "id": "10X filtering test",
            "workflowName": "CASAVA",
            "workflowVersion": "2.7.1",
            "parameters": {
                "--include-lane": "170915_D00353_0218_AHVGL2BCXY_lane_1,170915_D00353_0218_AHVGL2BCXY_lane_2,170919_D00343_0190_BCBKNDANXX_lane_1"
            }
        }, {
            "id": "Failed run test",
            "workflowName": "CASAVA",
            "workflowVersion": "2.7.1",
            "parameters": {
                "--include-lane": "180627_D00355_0229_ACCHCEANXX_lane_1"
            }
        }, {
            "id": "Failed run force run test",
            "workflowName": "CASAVA",
            "workflowVersion": "2.7.1",
            "parameters": {
                "--include-lane": "180627_D00355_0229_ACCHCEANXX_lane_1",
                "--disable-run-complete-check": "",
                "--lane-splitting": "true"
            }
        }, {
            "id": "Single end run test",
            "workflowName": "CASAVA",
            "workflowVersion": "2.7.1",
            "parameters": {
                "--include-lane": "110520_SN801_0050_AB09E1ABXX_lane_1",
                "--disable-run-complete-check": "",
                "--lane-splitting": "true"
            }
        }, {
            "id": "Do not provision out undetermined test",
            "workflowName": "CASAVA",
            "workflowVersion": "2.9.2",
            "parameters": {
                "--include-lane": "170627_D00343_0178_ACB0F2ANXX_lane_8",
                "--include-sample": "GCONT_0004_Li_C_PE_264_TS",
                "--demux-single-sample": "",
                "--provision-out-undetermined": "false"
            }
        }, {
            "id": "Run scanner no lane splitting test",
            "workflowName": "CASAVA",
            "workflowVersion": "2.9.1",
            "parameters": {
                "--include-sequencer-run": "180713_A00469_0010_AH7CT5DSXX",
                "--exclude-sample": "NOVA_0032_nn_U_PE_376_WG"
            }
        }
    ]
}
